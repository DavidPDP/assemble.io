---
layout: content
title: Releases
slug: releases
modifier: book
---
<!-- See `data/generated-pages/releases.yml` -->
{{#each releases.published}}
  <!-- {{title}} -->
  <div class="docs-section">
    <div class="page-header">
      <h1 id="{{@key}}">{{title}}</h1>
    </div>

    {{#each (expand files)}}
      <!-- {{basename .}} -->
      {{success '>> Inlining:' (basename .)}} {{! log a success message! }}

      {{#markdown}}
        {{inline .}}
      {{/markdown}}

      <br>
    {{/each}}

  </div>
{{/each}}