{{! hide the "content" navbar and related elements }}
<script>
  $( '.dismiss' ).click(function() {
    $( '.navbar-wrap' ).addClass( 'fade' );
    $( '.anchor-target' ).addClass( 'anchor-target-small' );
    $( '.navbar-wrap' ).delay(1000).hide( 'fast' );
  });
</script>


